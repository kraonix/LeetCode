class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        int n = nums.size();
        unordered_map<int, int> freq;

        for(int i : nums){
            freq[i]++;
        }

        vector<int> s;
        for(auto i : freq){
            if(i.second > 1){
                s.push_back(i.first);
            }
        }
        return s;
    }
};
