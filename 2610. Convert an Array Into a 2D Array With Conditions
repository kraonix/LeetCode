class Solution {
public:
    int sum(unordered_map<int, int> freq) {
        int s = 0;
        for (auto& p : freq) {
            s += p.second;
        }

        return s;
    }

    vector<vector<int>> findMatrix(vector<int>& nums) {
        vector<vector<int>> ans;
        unordered_map<int, int> freq;

        for (int i : nums) {
            freq[i]++;
        }

        while (true) {
            vector<int> temp;
            for (auto &p : freq) {
                if (p.second > 0) {
                    temp.push_back(p.first);
                    p.second--;
                }
            }

            ans.push_back(temp);
            if(sum(freq) <= 0){
                break;
            }
        }

        return ans;
    }
};
