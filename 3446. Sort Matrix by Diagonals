class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n = nums.size();
        int num, count = 0;

        
        for (auto x : nums) {
            if (count == 0)
                num = x;

            count += (x == num ? 1 : -1);
        }

        count = 0;
        for (auto x : nums)
            if (x == num)
                count++;

        if (count > n / 2)
            return num;

        return -1;
    }
};
