class Solution {
public:
    ListNode* insertGreatestCommonDivisors(ListNode* head) {
        ListNode* curr = head;

        while (curr != nullptr && curr->next != nullptr) {
            int g = gcd(curr->val, curr->next->val);

            ListNode* node = new ListNode(g);

            node->next = curr->next;
            curr->next = node;

            curr = node->next;
        }

        return head;
    }
};
